{
  "name": "@function-decorator/eslint",
  "type": "module",
  "version": "0.0.1",
  "description": "Eslint support for function decorator.",
  "author": "Naily Zero <zero@naily.cc> (https://naily.cc)",
  "keywords": [
    "es-project-template"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "watch": "tsup -w",
    "prepublishOnly": "tsup"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@function-decorator/oxc-parser": "workspace:*",
    "eslint": "^9.19.0",
    "magic-string": "^0.30.17",
    "oxc-parser": "^0.48.2"
  }
}
